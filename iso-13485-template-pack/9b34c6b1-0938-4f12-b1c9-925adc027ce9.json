      "type": "li",
      "indent": 0,
      "children": [
        {
          "id": "5f2c5cbf-91a1-423d-9d7c-3233e6532822",
          "type": "lic",
          "children": [
            {
              "text": "Hazard identification"
            }
          ]
        }
      ]
    },
